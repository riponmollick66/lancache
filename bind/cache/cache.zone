@       SOA     dns.ripon.com. root.dns.ripon.com. (
		2022112601 ; serial
                604800     ; refresh (1 week)
                600      ; retry (10 mins)
                600      ; expire (10 mins)
                600      ; minimum (10 mins)
                )
	IN NS	dns.ripon.com.

